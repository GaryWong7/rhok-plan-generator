<% title "Tasks" %>

<table>
  <tr>
    <th>Name</th>
    <th>Compulsory</th>
  </tr>
  <% for task in @tasks %>
    <tr>
      <td><%= task.name %></td>
      <td><%= task.compulsory %></td>
      <td><%= link_to "Show", task %></td>
      <td><%= link_to "Edit", edit_task_path(task) %></td>
    </tr>
  <% end %>
</table>

